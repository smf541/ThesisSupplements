begin mrbayes;
	set autoclose=yes nowarn=yes;

	
	[Char 64 has 6 or more states cannot be ordered in Mrbayes; 32 characters were ordered in O'Connor et al. SI, not 31 as stated in their text]
	CTYPE ordered:  1 3 8 28 31 43 51 56 [64] 67 69-70 72 74 92 107 117 159 168 176 183 193 197 205 213-214 216 219 222 229 233-234;
		
	LSET coding=variable rates=gamma;
	
	OUTGROUP Dromaeosauridae;
	constraint root =  1-. ;
	constraint birds =  2-. ;
	constraint Avebrevicauda = 3-. ; [ingroup]
	
	CALIBRATE	Dromaeosauridae			=fixed(111); [oldest of the three exemplars - Deinonychus, Velociraptor, Dromaeosaurus - used for coding this taxon]
